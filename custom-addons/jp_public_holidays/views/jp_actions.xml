<odoo>
  <record id="action_jp_holiday" model="ir.actions.act_window">
    <field name="name">Japan Holidays</field>
    <field name="res_model">jp.holiday</field>
    <field name="view_mode">list,form</field>
    <field name="view_id" ref="view_jp_holiday_list"/>
    <field name="search_view_id" ref="view_jp_holiday_search"/>
    <field name="context">
      {
        'search_default_current_year': 1,
        'default_year': 2025
      }
    </field>
  </record>
</odoo>
