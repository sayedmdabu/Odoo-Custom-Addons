<odoo>
  <record id="view_jp_holiday_filter_wizard" model="ir.ui.view">
    <field name="name">jp.holiday.filter.wizard.form</field>
    <field name="model">jp.holiday.filter.wizard</field>
    <field name="arch" type="xml">
      <form string="Filter by Year">
        <group>
          <field name="year"/>
        </group>
        <footer>
          <button name="action_apply_filter" type="object" string="Apply" class="btn-primary"/>
          <button string="Cancel" special="cancel" class="btn-secondary"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
